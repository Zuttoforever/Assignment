{
	"name": "twitter-fitness",
	"version": "1.0.0",
	"description": "",
	"main": "tw.js",
	"dependencies": {
		"async": "^3.2.2",
		"eslint-config-airbnb-base": "^13.1.0",
		"sqlite3": "^4.0.8",
		"twitter": "^1.7.1",
		"winston": "^3.2.1",
		"xo": "^0.24.0"
	},
	"private": true,
	"devDependencies": {},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"author": "",
	"license": "ISC",
	"xo": {
		"extends": "airbnb-base",
		"rules": {
			"spaced-comment": 0,
			"semi": [
				"error",
				"never"
			],
			"no-mixed-spaces-and-tabs": 1,
			"no-tabs": 0,
			"indent": [
				"error",
				"tab"
			],
			"no-underscore-dangle": 0,
			"comma-dangle": 0,
			"import/order": [
				"never"
			],
			"class-methods-use-this": 0,
			"arrow-parens": 0,
			"no-use-before-define": [
				"error",
				{
					"functions": false,
					"classes": false,
					"variables": true
				}
			],
			"quotes": 0,
			"operator-linebreak": [
				"error",
				"after"
			],
			"camelcase": 0,
			"no-plusplus": [
				"error",
				{
					"allowForLoopAfterthoughts": true
				}
			],
			"promise/prefer-await-to-then": 0,
			"unicorn/catch-error-name": [
				"error",
				{
					"name": "err"
				}
			],
			"linebreak-style": [
				"error",
				"unix"
			]
		}
	}
}
